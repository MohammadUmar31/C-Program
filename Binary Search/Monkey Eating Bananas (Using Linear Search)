    long long time_to_eat(int arr[], int n, int m){
        long long total_time=0;
        for(int i=0;i<n;i++){
            total_time=total_time+ceil((double)arr[i]/(double)m);
        }
        return total_time;
    }
    int minimumBananas(int arr[], int n, int h)
    {
        int m=1;
        int max=*max_element(arr,arr+n);
        for(m=1;m<=max;m++){
            if(time_to_eat(arr,n,m)<=h){
                return m;
            }
        }
        return -1;
    }
