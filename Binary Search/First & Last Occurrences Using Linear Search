void findFirstAndLast(int arr[], int n, int k) {
    int first = -1;
    int last = -1;
    for (int i = 0; i < n; ++i) {
        if (arr[i] == k) {
            if (first == -1) {
                first = i;
            }
            last = i;
        }
    }
    cout << first << " " << last << endl;
}
