    string removeOutermostParentheses(string s) {
        int count=0;
        string ans;
        for(auto ch : s){
            if(ch=='('){
                if(count!=0) ans.push_back(ch);
                count++;
            }
            else{
                count--;
                if(count!=0) ans.push_back(ch);
            }
        }
        return ans;
    }
